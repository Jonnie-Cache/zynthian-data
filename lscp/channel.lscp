
# Channel #chan#
ADD CHANNEL
SET CHANNEL AUDIO_OUTPUT_DEVICE #chan# 0
SET CHANNEL MIDI_INPUT_DEVICE #chan# 0
SET CHANNEL MIDI_INPUT_PORT #chan# 0
SET CHANNEL MIDI_INPUT_CHANNEL #chan# #chan#
LOAD ENGINE #engine# #chan#
LOAD INSTRUMENT NON_MODAL '#fpath#' 0 #chan#
SET CHANNEL AUDIO_OUTPUT_CHANNEL #chan# 0 0
SET CHANNEL AUDIO_OUTPUT_CHANNEL #chan# 1 1
SET CHANNEL VOLUME #chan# 1
SET CHANNEL MIDI_INSTRUMENT_MAP #chan# 0
