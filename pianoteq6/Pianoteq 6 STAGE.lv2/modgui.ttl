@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://pianoteq.com/lv2/Pianoteq6STAGE>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-pianoteq-6-stage.html> ;
        modgui:stylesheet <modgui/stylesheet-pianoteq-6-stage.css> ;
        modgui:screenshot <modgui/screenshot-pianoteq-6-stage.png> ;
        modgui:thumbnail <modgui/thumbnail-pianoteq-6-stage.png> ;
        modgui:brand "Modartt" ;
        modgui:label "Pianoteq 6 STAGE" ;
        modgui:model "boxy" ;
        modgui:panel "3-knobs" ;
        modgui:color "tribal2" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "lv2_port_19" ;
            lv2:name "Volume" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "lv2_port_2" ;
            lv2:name "Dynamics" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "lv2_port_4" ;
            lv2:name "Stereo Width" ;
        ] ;
    ] .